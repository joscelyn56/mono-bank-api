    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.9.2",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.0",
    "uuid": "^8.3.2"
  },
